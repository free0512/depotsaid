<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>calcul</servlet-name>
    <servlet-class>salaire.servlet.calculSalaire</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>assistante</servlet-name>
    <servlet-class>assistante.servlet.ficheAssistante</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>calcul</servlet-name>
    <url-pattern>/salaire</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>assistante</servlet-name>
    <url-pattern>/ficheAssistante</url-pattern>
  </servlet-mapping>
</web-app>